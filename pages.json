{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"navigationBarTextStyle":"white"
			}
		}
	    ,{
            "path" : "pages/stock-in/stockIn",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "入库查询",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#2c92fd",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color":"#8a8a8a",
		"selectedColor":"#66ccff",
		"list":[
			{
				"pagePath":"pages/index/index",
				"selectedIconPath":"static/iconfont/home1.png",
				"iconPath":"static/iconfont/home.png",
				"text":"首页"
			},
			{
				"pagePath":"pages/mine/mine",
				"selectedIconPath":"static/iconfont/person1.png",
				"iconPath":"static/iconfont/person.png",
				"text":"个人"
			}
		]
	}
}
